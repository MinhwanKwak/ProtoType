namespace BanpoFri.Data;



table UserData {
  money:string;
  storemoney:string;
  cash:int = 0;
  material:int = 0;
  facilitydatas:[FacilityData];
  lastlogintime:long = 0;
  curplaydatetime:long = 0;
}

table FacilityData {
  groundidx:int = 0;
  facilitygrade:int = 0; 
  landstatuseventidx:int = 0;
  landbenefittime:long = 0;
  iseventground:bool = false;
  isbenefit:bool = false;
}



root_type UserData;